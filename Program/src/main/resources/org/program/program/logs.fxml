<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.program.program.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <TableView fx:id="tableView" prefWidth="600" prefHeight="400">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" prefWidth="150"/>
            <TableColumn fx:id="nameColumn" text="Name" prefWidth="150"/>
            <TableColumn fx:id="actionColumn" text="Action" prefWidth="150"/>
            <TableColumn fx:id="textColumn" text="Text" prefWidth="150"/>
        </columns>
    </TableView>
    <Button text="Exit" onAction="#handleExit" prefWidth="150" prefHeight="25" />
</VBox>
